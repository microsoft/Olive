{
    "input_model": {
        "type": "OnnxModel",
        "model_path": "/tmp/simple_model.onnx"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device": "cpu",
                    "execution_providers": [
                        "CPUExecutionProvider"
                    ]
                }
            ]
        }
    },
    "data_configs": [
        {
            "name": "default_data_config",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "data_name": "dummy_data"
            },
            "pre_process_data_config": {
                "chat_template": false,
                "max_seq_len": 1024,
                "add_special_tokens": false,
                "max_samples": 256
            },
            "dataloader_config": {
                "batch_size": 1
            },
            "post_process_data_config": {}
        }
    ],
    "output_dir": "quantized-model",
    "host": "local_system",
    "target": "local_system",
    "no_artifacts": true,
    "cache_config": {
        "account_name": null,
        "container_name": null
    },
    "passes": {
        "aimetquantization": {
            "type": "AimetQuantization",
            "precision": "int8",
            "activation_type": "int8",
            "data_config": "default_data_config"
        }
    },
    "log_severity_level": 3
}