{
    "input_model": {
        "type": "HfModel",
        "model_path": "sentence-transformers/all-MiniLM-L6-v2",
        "task": "feature-extraction",
        "io_config": {
            "input_names": [ "input_ids", "token_type_ids", "attention_mask" ],
            "input_shapes": [ [ 1, 128 ], [ 1, 128 ], [ 1, 128 ] ],
            "input_types": [ "int64", "int64", "int64" ],
            "output_names": [ "output" ]
        }
    },
    "systems": {
        "cpu_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "cpu", "execution_providers": [ "CPUExecutionProvider" ] } ]
        },
        "qnn_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "npu", "execution_providers": [ "QNNExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "stsb_data_config",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "data_name": "sentence-transformers/stsb",
                "split": "test"
            },
            "pre_process_data_config": {
                "max_length": 128,
                "padding": "max_length",
                "input_cols": [ "sentence1" ],
                "label_col": "score"
            },
            "dataloader_config": { "batch_size": 1 }
        }
    ],
    "passes": {
        "conversion": { "device": "cpu", "type": "OnnxConversion", "target_opset": 17 },
        "QNNPreprocess": { "type": "QNNPreprocess", "fuse_layernorm": true },
        "OnnxQuantization": {
            "type": "OnnxQuantization",
            "data_config": "stsb_data_config",
            "activation_type": "QUInt16",
            "weight_type": "QUInt8",
            "calibrate_method": "MinMax",
            "quant_preprocess": true,
            "prepare_qnn_config": true
        }
    },
    "evaluate_input_model": false,
    "host": "cpu_system",
    "target": "cpu_system",
    "output_dir": "results/st"
}
