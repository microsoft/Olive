{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "generative": true,
            "io_config": {
                "input_names": [
                    "input_ids",
                    "attention_mask"
                ],
                "output_names": [
                    "logits"
                ],
                "input_shapes": [
                    [
                        2,
                        8
                    ],
                    [
                        2,
                        8
                    ]
                ],
                "input_types": [
                    "int32",
                    "int32"
                ],
                "kv_cache": false
            },
            "hf_config": {
                "model_name": "meta-llama/Llama-2-7b-hf",
                "model_class": "LlamaForCausalLM",
                "from_pretrained_args": {
                    "_attn_implementation": "eager"
                },
                "task": "text-generation"
            }
        }
    },
    "data_configs": [
        {
            "name": "wikitext2_train",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "params": {
                    "data_name": "wikitext",
                    "subset": "wikitext-2-raw-v1",
                    "split": "train"
                }
            },
            "pre_process_data_config": {
                "params": {
                    "text_cols": [
                        "text"
                    ],
                    "corpus_strategy": "join",
                    "add_special_tokens": false,
                    "source_max_len": 2048,
                    "max_samples": 128,
                    "joiner": "\n\n"
                }
            }
        },
        {
            "name": "transformer_prompt_dummy_data",
            "type": "TransformersPromptDummyDataContainer",
            "load_dataset_config": {
                "params": {
                    "generative": true
                }
            }
        },
        {
            "name": "transformer_token_dummy_data",
            "type": "TransformersTokenDummyDataContainer",
            "load_dataset_config": {
                "params": {
                    "generative": true
                }
            }
        }
    ],
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [
                            "CUDAExecutionProvider"
                        ]
                    }
                ]
            }
        }
    },
    "evaluators": {
        "merged_evaluator": {
            "metrics": [
                {
                    "name": "latency_prompt_processing",
                    "type": "latency",
                    "sub_types": [
                        {
                            "name": "avg",
                            "priority": 1
                        }
                    ],
                    "data_config": "transformer_prompt_dummy_data",
                    "user_config": {
                        "io_bind": true,
                        "run_kwargs": {
                            "max_new_tokens": 64
                        }
                    }
                }
            ]
        }
    },
    "passes": {},
    "auto_optimizer_config": {
        "disable_auto_optimizer": true
    },
    "engine": {
        "evaluator": "merged_evaluator",
        "host": "local_system",
        "target": "local_system",
        "cache_dir": "cache",
        "output_dir": "models/llama2_generate"
    }
}
