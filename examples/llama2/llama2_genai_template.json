{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "hf_config": {
                "model_name": "<model_name_placeholder>",
                "model_class": "LlamaForCausalLM",
                "task": "text-generation"
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [
                            "CUDAExecutionProvider"
                        ]
                    }
                ]
            }
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 16,
                "save_as_external_data": true,
                "all_tensors_to_one_file": true,
                "save_metadata_for_token_generation": true
            }
        },
        "exporter": {
            "type": "GenAIModelExporter",
            "config": {
                "precision": "int4",
                "search": {
                    "max_length": 2048,
                    "min_length": 0
                }
            }
        },
        "metadata": {
            "type": "GenAIModelExporter",
            "config": {
                "precision": "int4",
                "metadata_only": true,
                "search": {
                    "max_length": 2048,
                    "min_length": 0
                }
            }
        },
        "perf_tuning": {
            "type": "OrtPerfTuning",
            "config": {
                "user_script": "user_script.py",
                "dataloader_func": "dataloader_func_for_merged",
                "dataloader_func_kwargs": {
                    "model_id": "<model_name_placeholder>",
                    "past_seq_length": 0,
                    "seq_length": 8,
                    "max_seq_length": 2048
                },
                "batch_size": 2,
                "io_bind": true
            }
        }
    },
    "engine": {
        "packaging_config": [
            {
                "type": "Zipfile",
                "name": "OutputModel",
                "include_runtime_packages": false,
                "include_sample_code": false
            }
        ],
        "log_severity_level": 0,
        "host": "local_system",
        "target": "local_system",
        "cache_dir": "cache",
        "output_dir": null
    }
}
