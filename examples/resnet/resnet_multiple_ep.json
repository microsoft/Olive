{ "systems": {
        "python_system": {
            "type": "PythonEnvironment",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [ "DmlExecutionProvider", "OpenVINOExecutionProvider" ]
                    }
                ],
                "olive_managed_env": true,
                "requirements_file": "multiple_ep_requirements.txt"
            }
        }
    },
    "input_model": { "type": "ONNXModel", "config": { "model_path": "models/resnet_trained_for_cifar10.onnx" } },
    "data_configs": [
        {
            "name": "cifar10_data_config",
            "type": "HuggingfaceContainer",
            "user_script": "user_script.py",
            "load_dataset_config": { "type": "cifar10_dataset", "params": { "data_dir": "data" } },
            "pre_process_data_config": { "type": "skip_pre_process" },
            "dataloader_config": { "params": { "batch_size": 1, "drop_last": true } }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [
                        {
                            "name": "accuracy_score",
                            "priority": 1, "metric_config": { "task": "multiclass", "num_classes": 1000 }
                        }
                    ],
                    "user_config": {
                        "post_processing_func": "post_process",
                        "user_script": "user_script.py",
                        "dataloader_func": "create_dataloader",
                        "batch_size": 1,
                        "data_dir": "data"
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [ { "name": "avg", "priority": 2 } ],
                    "user_config": {
                        "user_script": "user_script.py",
                        "dataloader_func": "create_dataloader",
                        "batch_size": 1,
                        "data_dir": "data"
                    }
                }
            ]
        }
    },
    "passes": {
        "onnx_quantization": {
            "type": "OnnxQuantization",
            "config": {
                "user_script": "user_script.py",
                "data_dir": "data",
                "dataloader_func": "resnet_calibration_reader",
                "weight_type": "QUInt8",
                "activation_type": "QUInt8",
                "quant_preprocess": true
            }
        },
        "perf_tuning": { "type": "OrtPerfTuning", "config": { "data_config": "cifar10_data_config" } }
    },
    "engine": {
        "evaluator": "common_evaluator",
        "target": "python_system",
        "host": "python_system",
        "cache_dir": "cache",
        "output_dir": "models/resnet_multiple_ep"
    }
}
