{
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_loader": "load_argmax_sampling_model",
            "model_script": "user_script.py",
            "io_config": {
                "input_names": [ "logits" ],
                "output_names": [ "next_token" ],
                "dynamic_axes": {
                    "logits": { "0": "batch_size" }
                }
            },
            "dummy_inputs_func": "argmax_sampling_inputs"
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [
                            "DmlExecutionProvider"
                        ]
                    }
                ]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg"}],
                    "user_config": {
                        "user_script": "user_script.py",
                        "dataloader_func": "argmax_sampling_data_loader",
                        "batch_size": 1
                    }
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 17
            }
        }
    },
    "engine": {
        "evaluator": "common_evaluator",
        "evaluate_input_model": false,
        "host": "local_system",
        "target": "local_system",
        "cache_dir": "cache",
        "output_name": "argmax_sampling",
        "output_dir": "footprints"
    }
}
