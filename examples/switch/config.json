{
    "verbose": true,
    "input_model":{
        "type": "OnnxModel",
        "config": {
            "model_path": "model_4n_2l_8e.onnx"
        }
    },
    "passes": {
        "distribute": {
            "type": "MoEExpertsDistributor",
            "config": {
                "world_size": 2
            }
        }
    },
    "systems": {
