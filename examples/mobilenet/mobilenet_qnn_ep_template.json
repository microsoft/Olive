{
    "input_model": { "type": "OnnxModel", "model_path": "models/mobilenetv2-12.onnx" },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "execution_providers": [ "QNNExecutionProvider" ] } ]
        },
        "qnn_ep_env": {
            "type": "IsolatedORT",
            "python_environment_path": "<qnn_env_path>",
            "accelerators": [ { "execution_providers": [ "QNNExecutionProvider" ] } ],
            "preprend_to_path": [ "<qnn_lib_path>" ]
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [
                        {
                            "name": "accuracy_score",
                            "priority": 1,
                            "metric_config": { "task": "multiclass", "num_classes": 1000 }
                        }
                    ],
                    "user_config": {
                        "user_script": "user_script.py",
                        "data_dir": "data/eval",
                        "batch_size": 1,
                        "dataloader_func": "evaluation_dataloader",
                        "post_processing_func": "post_process"
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [ { "name": "avg", "priority": 2 } ],
                    "user_config": {
                        "user_script": "user_script.py",
                        "data_dir": "data/eval",
                        "batch_size": 1,
                        "dataloader_func": "evaluation_dataloader"
                    }
                }
            ]
        }
    },
    "passes": {
        "qnn_preprocess": { "type": "QNNPreprocess" },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "user_script": "user_script.py",
            "dataloader_func": "mobilenet_calibration_reader",
            "data_dir": "data/eval",
            "activation_type": "QUInt16",
            "weight_type": "QUInt8"
        }
    },
    "target": "<target>",
    "evaluator": "<evaluator>",
    "evaluate_input_model": false,
    "cache_dir": "cache",
    "output_dir": "models/mobilenet_qnn_ep"
}
