{
    "input_model":{
        "type": "OnnxModel",
        "config": {
            "model_path": "models/mobilenetv2-12.onnx"
        }
    },
    "evaluators": {
        "common_evaluator":{
            "metrics":[
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [{"name": "accuracy_score", "priority": 1}],
                    "user_config":{
                        "user_script": "user_script.py",
                        "data_dir": "data/eval",
                        "batch_size": 1,
                        "dataloader_func": "evaluation_dataloader",
                        "post_processing_func": "post_process"

                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg", "priority": 2}],
                    "user_config":{
                        "user_script": "user_script.py",
                        "data_dir": "data/eval",
                        "batch_size": 1,
                        "dataloader_func": "evaluation_dataloader"
                    }
                }
            ]
        }
    },
    "passes": {
        "quantization": {
            "type": "OnnxStaticQuantization",
            "config": {
                "user_script": "user_script.py",
                "dataloader_func": "mobilenet_calibration_reader",
                "data_dir": "data/quant",
                "quant_format": "QDQ",
                "per_channel": false
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "search_strategy": true,
        "cache_dir": "cache",
        "output_dir": "models",
        "evaluator": "common_evaluator",
        "evaluate_input_model": false,
        "clean_cache": true,
        "target": {
            "type": "PythonEnvironment",
            "config": {
                "python_environment_path": "<python-environment-path>",
                "accelerators": ["npu"],
                "prepend_to_path": ["<qnn-lib-path>"]
            }
        }
}
}
