{
    "input_model": {
        "type": "TensorFlowModel",
        "config": {
            "model_path": "models/inception_v3.pb"
        }
    },
    "data_configs": [
        {
            "name": "raw_data",
            "type": "RawDataContainer",
            "user_script": "user_script.py",
            "load_dataset_config": {
                "params": {
                    "data_dir": "data",
                    "input_names": [
                        "input"
                    ],
                    "input_shapes": [
                        [
                            1,
                            299,
                            299,
                            3
                        ]
                    ],
                    "input_order_file": "input_order.txt",
                    "annotations_file": "labels.npy"
                }
            },
            "post_process_data_config": {
                "type": "inception_post_process"
            },
            "dataloader_config": {
                "params": {
                    "batch_size": 7
                }
            }
        }
    ],
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "cpu"
                    }
                ]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [
                        {
                            "name": "accuracy_score",
                            "priority": 1,
                            "metric_config": {
                                "task": "multiclass",
                                "num_classes": "100",
                                "top_k": 1
                            }
                        }
                    ],
                    "user_config": {
                        "inference_settings": {
                            "snpe": {
                                "return_numpy_results": true
                            }
                        }
                    },
                    "data_config": "raw_data"
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [
                        {
                            "name": "avg",
                            "priority": 2,
                            "metric_config": {
                                "warmup_num": 0,
                                "repeat_test_num": 5,
                                "sleep_num": 2
                            }
                        }
                    ],
                    "user_config": {
                        "inference_settings": {
                            "snpe": {
                                "return_numpy_results": true,
                                "perf_profile": "sustained_high_performance",
                                "profiling_level": "moderate"
                            }
                        }
                    },
                    "data_config": "raw_data"
                }
            ]
        }
    },
    "passes": {
        "snpe_conversion": {
            "type": "SNPEConversion",
            "config": {
                "input_names": [
                    "input"
                ],
                "input_shapes": [
                    [
                        1,
                        299,
                        299,
                        3
                    ]
                ],
                "output_names": [
                    "InceptionV3/Predictions/Reshape_1"
                ]
            }
        },
        "snpe_quantization": {
            "type": "SNPEQuantization",
            "config": {
                "enable_htp": true,
                "data_config": "raw_data"
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "evaluate_input_model": false,
        "clean_cache": true,
        "target": "local_system",
        "host": "local_system",
        "evaluator": "common_evaluator",
        "cache_dir": "cache",
        "output_dir": "outputs",
        "output_name": "snpe_quantized"
    }
}
