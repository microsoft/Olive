{
    "input_model":{
        "type": "TensorFlowModel",
        "config": {
            "model_path": "models/inception_v3.pb"
        }
    },
    "data_configs": {
        "raw_data": {
            "name": "raw_data",
            "type": "RawDataContainer",
            "params_config": {
                "data_dir": "data",
                "input_names": ["input"],
                "input_shapes": [[1, 299, 299, 3]],
                "input_order_file": "input_order.txt",
                "annotations_file": "labels.npy",
                "batch_size": 7
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics":[
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [
                        {"name": "accuracy_score", "priority": 1}
                    ],
                    "user_config":{
                        "user_script": "user_script.py",
                        "post_processing_func": "post_process",
                        "inference_settings": {
                            "snpe": {
                                "return_numpy_results": true
                            }
                        }
                    },
                    "data_config": "raw_data"
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [
                        {"name": "avg", "priority": 2, "metric_config": {"warmup_num": 0, "repeat_test_num": 5, "sleep_num": 2}}
                    ],
                    "user_config": {
                        "inference_settings": {
                            "snpe": {
                                "return_numpy_results": true,
                                "perf_profile": "sustained_high_performance",
                                "profiling_level": "moderate"
                            }
                        }
                    },
                    "data_config": "raw_data"
                }
            ]
        }
    },
    "passes": {
        "snpe_conversion": {
            "type": "SNPEConversion",
            "config": {
                "input_names": ["input"],
                "input_shapes": [[1, 299, 299, 3]],
                "output_names": ["InceptionV3/Predictions/Reshape_1"]
            }
        },
        "snpe_quantization": {
            "type": "SNPEQuantization",
            "config": {
                "enable_htp": true,
                "data_config": "raw_data"
            }
        }
    },
    "engine": {
        "search_strategy": false,
        "evaluator": "common_evaluator",
        "cache_dir": "cache",
        "output_dir" : "outputs",
        "output_name": "snpe_quantized"
    }
}
