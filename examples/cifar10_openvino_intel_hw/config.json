{
    "verbose": true,
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_path": "chenyaofo/pytorch-cifar-models",
            "model_storage_kind": "file",
            "model_loader": "load_pytorch_origin_model",
            "model_script": "{USER_SCRIPT}"
        }
    },
    "systems": {
        "local_system": {"type": "LocalSystem"}
    },
    "evaluators": {
        "common_evaluator": {
            "metrics":[
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_type": "accuracy_score",
                    "priority_rank": 1,
                    "user_config":{
                        "post_processing_func": "post_process",
                        "user_script": "{USER_SCRIPT}",
                        "dataloader_func": "create_dataloader",
                        "data_dir": "data",
                        "batch_size": 1
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_type": "avg",
                    "priority_rank": 2,
                    "user_config":{
                        "user_script": "{USER_SCRIPT}",
                        "dataloader_func": "create_dataloader",
                        "data_dir": "data",
                        "batch_size": 1
                    }
                }
            ],
            "target": "local_system"
        }
    },
    "passes": {
        "conversion": {
            "type": "OpenVINOConversion",
            "config": {
                "input_shape": [1, 3, 32, 32]
            },
            "host": "local_system"
        },
        "quantization": {
            "type": "OpenVINOQuantization",
            "config": {
                "engine_config": {"device": "CPU", "stat_requests_number": 2, "eval_requests_number": 2},
                "algorithms": [
                    {
                        "name": "DefaultQuantization",
                        "params": {"target_device": "CPU", "preset": "performance", "stat_subset_size": 300}
                    }
                ],
                "data_dir": "data",
                "user_script": "{USER_SCRIPT}",
                "dataloader_func": "create_dataloader"
            },
            "host": "local_system"
        }
    },
    "engine": {
        "search_strategy": {
            "execution_order": "pass-by-pass",
            "search_algorithm": "random"
        },
        "evaluator": "common_evaluator",
        "host": {"type": "LocalSystem"},
        "cache_dir": "cache"
    }
}
