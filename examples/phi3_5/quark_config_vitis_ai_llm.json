{
    "input_model": {
        "type": "HFModel",
        "model_path": "microsoft/Phi-3.5-mini-instruct"
    },
    "passes": {
        "qq": {
            "type": "QuarkQuantizationPass",
            "quant_scheme": "w_uint4_per_group_asym",
            "quant_algo": "awq",
            "dataset": "pileval_for_awq_benchmark",
            "data_type": "float32",
            "num_calib_data": 128,
            "model_export": ["hf_format"],
            "exclude_layers": []
        },
        "mg": {
            "type": "VitisGenerateModelLLM",
            "packed_const": false,
            "cpu_only": false
        }
    },
    "log_severity_level": 1,
    "output_dir": "models/phi3_5-vai",
    "cache_dir": "cache",
    "no_artifacts": true
}
