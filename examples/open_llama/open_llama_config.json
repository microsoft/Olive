{
    "input_model":{
        "type": "OptimumModel",
        "config": {
            "model_path": "openlm-research/open_llama_3b",
            "model_components": ["decoder_model.onnx", "decoder_with_past_model.onnx"]
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": ["cpu"]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg"}]
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OptimumConversion",
            "config": {
                "target_opset": 14,
                "save_as_external_data": true,
                "all_tensors_to_one_file": true
            }
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "gpt2",
                "float16": true,
                "use_gpu": false,
                "keep_io_types": true,
                "num_heads": 32,
                "hidden_size": 4096,
                "optimization_options": {
                    "use_multi_head_attention": false
                }
            }
        },
        "merge": {
            "type": "OptimumMerging",
            "config": {
            }
        }
    },
    "engine": {
        "evaluator": "common_evaluator",
        "cache_dir": "cache",
        "output_name": "ollama",
        "output_dir": "footprints"
    }
}
