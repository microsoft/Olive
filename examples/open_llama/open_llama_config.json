{
    "input_model": { "type": "HfModel", "model_path": "openlm-research/open_llama_3b" },
    "data_configs": [ { "name": "transformer_token_dummy_data", "type": "TransformersTokenDummyDataContainer" } ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [ { "name": "avg" } ],
                    "data_config": "transformer_token_dummy_data"
                }
            ]
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "gpu", "execution_providers": [ "CUDAExecutionProvider" ] } ]
        }
    },
    "passes": {
        "conversion_merged": {
            "type": "OnnxConversion",
            "target_opset": 15,
            "save_as_external_data": true,
            "all_tensors_to_one_file": true
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "model_type": "gpt2",
            "float16": true,
            "use_gpu": true,
            "keep_io_types": true,
            "num_heads": 32,
            "hidden_size": 4096,
            "optimization_options": { "use_multi_head_attention": false }
        }
    },
    "evaluator": "common_evaluator",
    "cache_dir": "cache",
    "output_name": "ollama",
    "host": "local_system",
    "target": "local_system",
    "output_dir": "models/open_llama"
}
