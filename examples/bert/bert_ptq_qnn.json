{
    "input_model": {
        "type": "PyTorchModel",
        "model_loader": "load_bert",
        "model_script": "user_script_qnn.py",
        "model_path": "Intel/bert-base-uncased-mrpc",
        "model_attributes": {
            "task": "text-classification",
            "extended_mask_type": "non-causal",
            "extended_mask_value": -100
        },
        "io_config": {
            "input_names": [ "input_ids", "attention_mask", "token_type_ids" ],
            "input_shapes": [ [ 1, 128 ], [ 1, 1, 128, 128 ], [ 1, 128 ] ],
            "input_types": [ "int64", "float32", "int64" ],
            "output_names": [ "logits" ],
            "output_shapes": [ [ 1, 2 ] ],
            "output_types": [ "float32" ]
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "npu", "execution_providers": [ "QNNExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "glue_mrpc",
            "type": "HuggingfaceContainer",
            "load_dataset_config": { "data_name": "glue", "subset": "mrpc", "split": "validation" },
            "pre_process_data_config": {
                "max_samples": 10,
                "max_length": 128,
                "padding": "max_length",
                "input_cols": [ "sentence1", "sentence2" ]
            },
            "dataloader_config": { "batch_size": 1 }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "data_config": "glue_mrpc",
                    "sub_types": [ { "name": "accuracy_score", "priority": 1 } ]
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "data_config": "glue_mrpc",
                    "sub_types": [ { "name": "avg", "priority": 2 } ]
                }
            ]
        }
    },
    "passes": {
        "conversion": { "type": "OnnxConversion", "target_opset": 17 },
        "qnn_preprocess": { "type": "QNNPreprocess" },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "data_config": "glue_mrpc",
            "activation_type": "QUInt16",
            "weight_type": "QUInt8"
        }
    },
    "evaluator": "common_evaluator",
    "evaluate_input_model": false,
    "host": "local_system",
    "target": "local_system",
    "cache_dir": "cache",
    "output_dir": "models/bert_ptq_qnn"
}
