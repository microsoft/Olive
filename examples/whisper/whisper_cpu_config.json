    "systems": {
        "local_system": {"type": "LocalSystem"}
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 13
            }
        },
        "transformers_optimization": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "whisper",
                "num_heads": 6,
                "hidden_size": 384
            }
        },
        "onnx_dynamic_quantization": {
            "type": "OnnxDynamicQuantization",
            "disable_search": true,
            "config": {
                "weight_type": "QUInt8",
                "per_channel": false,
                "reduce_range": false,
                "op_types_to_quantize": ["MatMul", "Gemm"],
                "MatMulConstBOnly": false
            },
            "clean_run_cache": false
        }
    },
    "engine": {
        "search_strategy": false,
        "host": "local_system",
        "clean_cache": true,
        "cache_dir": "cache"
