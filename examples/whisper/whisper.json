{
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_script": "code/user_script.py",
            "script_dir": "code",
            "hf_config": {
                "task" : "automatic-speech-recognition",
                "model_name" : "openai/whisper-tiny.en"
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    "cpu"
                ]
            }
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 17
            }
        },
        "transformers_optimization": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "bart",
                "num_heads": 4,
                "hidden_size": 256,
                "optimization_options": {
                    "use_multi_head_attention": true
                },
                "use_gpu": false
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "search_strategy": false,
        "host": "local_system",
        "target": "local_system",
        "clean_cache": true,
        "cache_dir": "cache",
        "output_dir": "models",
        "output_name": "whisper"
    }
}