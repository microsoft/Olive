{
    "input_model": {
      "type": "HfModel",
        "model_path": "microsoft/table-transformer-detection",
        "task": "object-detection",
        "io_config": {
            "input_names": [ "input" ],
            "input_shapes": [ [1, 3, 800, 800] ],
            "output_names": [ "logits", "pred_boxes" ]
        }
    },
    "systems": {
        "cpu_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "cpu", "execution_providers": [ "CPUExecutionProvider" ] } ]
        },
        "qnn_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "npu", "execution_providers": [ "QNNExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "tablebank_data_config",
            "user_script": "ttd.py",
            "load_dataset_config": { 
                "type": "dataset_load",
                "data_dir": "tablebank_test"
             },
            "post_process_data_config": { "type": "dataset_post_process" }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "custom",
                    "data_config": "tablebank_data_config",
                    "sub_types": [
                      {
                        "name": "MeanOfMaxIoU",
                        "higher_is_better": true
                      }
                    ],
                    "user_config": {
                      "user_script": "ttd.py",
                      "metric_func": "evaluate"
                    }
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "device": "cpu",
            "type": "OnnxConversion",
            "target_opset": 17,
            "save_as_external_data": true
        },
        "QNNPreprocess": {
             "type": "QNNPreprocess",
              "fuse_layernorm": true,
            "save_as_external_data": true
            },
        "OnnxQuantization": {
            "type": "OnnxQuantization",
            "data_config": "tablebank_data_config",
            "op_types_to_quantize": [ "MatMul", "LayerNormalization", "Gemm", "Sigmoid", "Gelu" ],
            "activation_type": "QUInt16",
            "weight_type": "QUInt8",
            "calibrate_method": "MinMax",
            "quant_preprocess": true,
            "prepare_qnn_config": true,
            "save_as_external_data": true
        }
    },
    "evaluator": "common_evaluator",
    "evaluate_input_model": false,
    "host": "cpu_system",
    "target": "cpu_system",
    "output_dir": "results/ttd"
}
  