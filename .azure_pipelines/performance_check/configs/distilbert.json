{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "hf_config": {
                "model_name": "distilbert-base-uncased-finetuned-sst-2-english",
                "task": "text-classification"
            }
        }
    },
    "data_configs": [
        {
            "name": "glue_sst2",
            "type": "HuggingfaceContainer",
            "load_dataset_config": { "params": { "data_name": "glue", "subset": "sst2", "split": "validation" } },
            "pre_process_data_config": {
                "params": { "input_cols": [ "sentence" ], "label_cols": [ "label" ], "max_samples": 100 }
            },
            "dataloader_config": { "params": { "batch_size": 1 } }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "backend": "huggingface_metrics",
                    "data_config": "glue_sst2",
                    "sub_types": [
                        { "name": "accuracy", "priority": 1, "goal": { "type": "max-degradation", "value": 0.01 } }
                    ]
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "data_config": "glue_sst2",
                    "sub_types": [
                        { "name": "avg", "priority": 2, "goal": { "type": "percent-min-improvement", "value": 20 } }
                    ]
                }
            ]
        }
    },
    "passes": {
        "conversion": { "type": "OnnxConversion", "config": { "target_opset": 13 } },
        "transformers_optimization": { "type": "OrtTransformersOptimization", "disable_search": true },
        "quantization": {
            "type": "OnnxQuantization",
            "config": { "quant_preprocess": true, "data_config": "glue_sst2" }
        },
        "perf_tuning": { "type": "OrtPerfTuning", "config": { "data_config": "glue_sst2" } }
    },
    "engine": {
        "search_strategy": {
            "execution_order": "joint",
            "search_algorithm": "tpe",
            "search_algorithm_config": { "num_samples": 3, "seed": 0 }
        },
        "log_severity_level": 0,
        "clean_cache": true,
        "evaluator": "common_evaluator",
        "cache_dir": "run_cache/olive/cache",
        "output_dir": "run_cache/olive/distilbert"
    }
}
